//==============================================

//счётчик
push_int (ofs 0) (int 0)

//шаг
push_int (ofs 4) (int 1)

//цель
push_int (ofs 8) (int 1)
push_int (ofs 12) (int 1000000)

//наш цикл
label l1
	add_int (ofs 0) (ofs 4)
	mov_int (ofs 16) (ofs 0)
	//out_int 0
eq_int (ofs 16), (ofs 8)
jf (ofs 16) l1

//выводим результат
out_int	(ofs 0)

wait
ret